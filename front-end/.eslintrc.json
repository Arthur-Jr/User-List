
{
  "root": true,
  "extends": ["airbnb-base", "eslint:recommended", "react-app"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "no-console":0,
    "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
  }
}