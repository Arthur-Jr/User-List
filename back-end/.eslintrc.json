{
  "root": true,
  "extends": ["airbnb-base", "eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-console":0,
    "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
  }
}